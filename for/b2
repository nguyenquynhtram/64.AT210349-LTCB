#include <stdio.h>

int main()
{
  int i ,n;
	printf("nhap vao so bat ky: ");
	scanf("%d",&n);
	
	if (n==1)
	    printf("%d k phai la so nguyen to",n);
	else{
	    for(i = 2;i < n;i++){
		    if (n%i==0){
		         printf("%d k phai la so nguyen to",n);
			       return 0;
		    }
		  }
		  printf("%d la so nguyen to",n);
  }
}
