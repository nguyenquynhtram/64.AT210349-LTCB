#include <stdio.h>

int main()
{
    int i,a,b,ucln,bcnn,tg;
    printf("nhap vao 2 so nguyen a va b: ");
    scanf("%d%d",&a,&b);
    
    if (a > b){
        tg = a;
        a = b;
        b = tg;
    }
    
    for(i = a; i >= 1;i--){
        if (a%i==0 && b%i==0){
            ucln = i ;
            break;
        }
    }
    printf("ucln = %d",ucln);
    bcnn = (a*b)/ucln;
    printf("\nbcnn = %d",bcnn);
}
