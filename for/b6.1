#include <stdio.h>

int main()
{
    int i,a,b,ucln,bcnn;
    printf("nhap vao 2 so nguyen a va b: ");
    scanf("%d%d",&a,&b);
    
    for(i = 2;i <= a || i <= b;i++){
        if (a%i==0 && b%i==0){
            ucln = i ;
        }
    }
    printf("ucln = %d",ucln);
    bcnn = (a*b)/ucln;
    printf("\nbcnn = %d",bcnn);
}
