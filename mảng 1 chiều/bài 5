#include <stdio.h>

int main()
{
    int a[101],i,n,x,dem=0;
    
    printf("nhap vao phan tu x: ");
    scanf("%d",&x);
    printf("nhap vao so phan tu cua mang: ");
    scanf("%d",&n);
    
    for(i = 0;i < n;i++){
        printf("nhap vao phan tu thu %d: ",i);
        scanf("%d",&a[i]);
    }
    
    for(i = 0;i < n;i++){
        if(a[i]==x){
            dem += 1;
        }
    }
    printf("phan tu x xuat hien %d lan trong mang",dem);
}
