#include <stdio.h>

long luythua(int,int);

long luythua(int a,int b ){
    int i ;
    long s = 1;
    for (i = 1;i <= b;i++){
        s = s*a;
    }
    return s;
}

int main(){
    int i,n;
    long s = 0;
    printf("nhap vao n: ");
    scanf("%d",&n);
    
    for (i = 1;i <= n;i++){
        s += luythua(i,i);
    }
    printf("%ld",s);
}
